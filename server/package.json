{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "dev": "dotenv -e .env.dev -- nest start --watch",
    "dev:linux": "NODE_ENV=development nest start --watch",
    "dev:linux2": "nest start --watch",
    "prod": "SET NODE_ENV=production& nest start",
    "prod:linux": "NODE_ENV=production nest start",
    "db-push:dev": "dotenv -e .env.dev -- pnpx prisma db push",
    "db-push:prod": "dotenv -e .env.prod -- npx prisma db push",
    "db-pull:dev": "dotenv -e .env.dev -- pnpx prisma db pull",
    "db-pull:prod": "dotenv -e .env.prod -- npx prisma db pull",
    "db-generate:dev": "dotenv -e .env.dev -- pnpx prisma generate",
    "db-generate:dev:linux": "dotenv -e .env.dev -- && pnpx prisma generate",
    "db-generate:prod": "dotenv -e .env.prod -- npx prisma generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.1",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.1",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "4.15.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "exceljs": "^4.3.0",
    "moment": "^2.29.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0",
    "webpack": "^5.87.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.2",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
